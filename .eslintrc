{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "generators": true,
      "modules": true,
      "objectLiteralDuplicateProperties": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0
  }
}
